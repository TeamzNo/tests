local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Londons Calling Script", "Sentinel")

--MAIN
local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Main (not bait)")
    

MainSection:NewButton("ESP", "Includes health and other features", function()
    loadstring(game:HttpGet(("https://raw.githubusercontent.com/ic3w0lf22/Unnamed-ESP/master/UnnamedESP.lua"), true))()
end)

MainSection:NewButton("Chat spy (/spy to disable/toggle)", "Allows you to see all games chats", function()
    loadstring(game:HttpGet(("https://pastebin.com/raw/MwmrpsPK"), true))()
end)

MainSection:NewButton("Autoclicker", "Good for knives", function()
    loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/JustEzpi/ROBLOX-Scripts/main/ROBLOX_AutoClicker"))()
end)

MainSection:NewButton("Refill hunger", "No cost and back to 100pc", function()
    if true then game.Players.LocalPlayer.Hunger.Value = 100 wait() end
end)

local MainSection = Main:NewSection("Main (bait, dont do when mods r around)")

 MainSection:NewSlider("Walkspeed", "SPEED!!", 200, 16, function(s)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
    end)


    MainSection:NewButton("Reset WS", "Resets to all defaults", function()
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
    end)
    
MainSection:NewButton("Fly (Left Alt to toggle)", "so bait but fun in own server", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/75U6zUmq", true))()
end)
    
--TPs

